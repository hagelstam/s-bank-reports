{
  "name": "s-bank-reports",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next start",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "dev": "next dev",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "clean": "rm -rf .next node_modules",
    "docker:prod:up": "docker compose -f compose.prod.yml up --build",
    "docker:prod:down": "docker compose -f compose.prod.yml down",
    "docker:dev:up": "docker compose -f compose.dev.yml up --build",
    "docker:dev:down": "docker compose -f compose.dev.yml down"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^15.2.0",
    "clsx": "^2.1.1",
    "next": "15.2.3",
    "papaparse": "^5.5.2",
    "pdf-lib": "^1.17.1",
    "prom-client": "^15.1.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@tailwindcss/postcss": "^4.0.9",
    "@types/node": "^22.13.8",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "cypress": "^14.1.0",
    "eslint": "^9.21.0",
    "eslint-config-next": "15.2.0",
    "eslint-config-prettier": "^10.0.2",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "start-server-and-test": "^2.0.10",
    "tailwindcss": "^4.0.9",
    "typescript": "^5.8.2"
  }
}
